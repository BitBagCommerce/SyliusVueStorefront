{
  "name": "@vue-storefront/sylius-theme",
  "version": "1.0.2",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build -m",
    "build:analyze": "nuxt build -a -m",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test:e2e": "cd tests/e2e/ && npx cypress open --config-file cypress.config.js",
    "test:e2e:hl": "cd tests/e2e/ && npx cypress run --headless --config-file cypress.config.js",
    "test:e2e:generate:report": "yarn -s mochawesome-merge \"tests/e2e/report/*.json\" > \"tests/e2e/report.json\" && yarn -s marge tests/e2e/report.json -o \"tests/e2e/report\""
  },
  "dependencies": {
    "@nuxtjs/style-resources": "^1.2.1",
    "@realtainment/sylius": "^1.1.5",
    "@storefront-ui/vue": "^0.13.3",
    "@vue-storefront/middleware": "^2.7.1",
    "@vue-storefront/nuxt": "^2.7.1",
    "@vue-storefront/nuxt-theme": "^2.7.1",
    "atob": "^2.1.2",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^2.6.12",
    "date-fns": "^2.29.3",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.28.1",
    "vee-validate": "^3.4.5",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@nuxt/types": "^0.7.9",
    "@nuxt/typescript-build": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "cypress": "^12.13.0",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "^1.1.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.8.4"
  }
}